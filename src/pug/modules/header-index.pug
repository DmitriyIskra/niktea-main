    .carousel-wrap 
        #carouselExampleCaptions.carousel.slide
            .carousel-inner
                .carousel-item.active
                    img.d-block.w-100(src='../img/content/slide-22.png' alt='...')
                .carousel-item
                    img.d-block.w-100(src='../img/content/slide-1.png' alt='...')
            button.carousel-control-prev(type='button' data-bs-target='#carouselExampleCaptions' data-bs-slide='prev')
